<!DOCTYPE html>
<html>
<head>
  <title>Cabservice</title>
  <%= stylesheet_link_tag 'bootstrap.min.css','custom', :media => 'screen' %>
  
  <%= javascript_include_tag 'https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js', 'bootstrap.min.js','data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>

<body id="myPage" data-spy="scroll" data-target=".navbar" data-offset="60">
<nav class="navbar navbar-inverse navbar-fixed-top">
  <div class="container">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span> 
      </button>
      <%= link_to "Fuber", root_url, :class => "navbar-brand" %>
    </div>
    <div class="collapse navbar-collapse" id="myNavbar">	   
      <ul class="nav navbar-nav navbar-right">
        <li ><a href="#about">ABOUT</a></li>
        <li><a href="#contact">CONTACT</a></li>
        <li>
	 </li>
      </ul>
    </div>
  </div>
</nav>


<% flash.each do |name, msg| %>
  <%= content_tag :div, msg,:class=>flash, :id => "flash_#{name}" %>
<% end %>



<%= yield %>



</body>
</html>
